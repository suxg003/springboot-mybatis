CREATE TABLE `carinfo` (
  `id` int(20) NOT NULL AUTO_INCREMENT COMMENT 'id',
  `cartype` varchar(20) DEFAULT NULL COMMENT '汽车类型',
  `carnumber` varchar(20) DEFAULT NULL COMMENT '车牌号',
  `carpp` varchar(30) DEFAULT '' COMMENT '品牌',
  `carcjh` varchar(300) DEFAULT '' COMMENT '车架号',
  `persionid` varchar(30) DEFAULT '' COMMENT '身份证号',
  `persionname` varchar(30) DEFAULT '' COMMENT '姓名',
  `gctime` varchar(100) DEFAULT '' COMMENT '购车日期',
  `addessid` varchar(300) DEFAULT '' COMMENT '联系地址',
  `phone` varchar(30) DEFAULT '' COMMENT '手机号',
  `createtime` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  `updatetime` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=34556 DEFAULT CHARSET=utf8 COMMENT='车辆信息'
